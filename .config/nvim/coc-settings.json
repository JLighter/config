{
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.snippets.enable": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "suggest.maxCompleteItemCount": 15,
  "suggest.localityBonus": true,
  "suggest.fixInsertedWord": true,
  "suggest.triggerCompletionWait": 100,
  "suggest.autoTrigger": "always",
  "suggest.disableMenu": true,
  "suggest.snippetIndicator": "",
  "suggest.floatEnable": true,
  "suggest.enablePreselect": true,
  "suggest.enablePreview": true,
  "suggest.noselect": false,
  "suggest.echodocSupport": true,
  "diagnostic.enable": true,
  "diagnostic.displayByAle": false,
  "signature.enable": true,
  "git.enableGutters": false,
  "git.realtimeGutters": false,
  "tsserver.enableJavascript": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "dart"
  ],
  "typescript.referencesCodeLens.enable": false,
  "typescript.preferences.quoteStyle": "single",
  "javascript.preferences.quoteStyle": "single",
  "tslint.suppressWhileTypeErrorsPresent": true,
  "tslint.autoFixOnSave": false,
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "django",
    "blade"
  ]
}
