[user]
	email = heroguelle.julien@gmail.com
	name = JLighter
[alias]
	quickshare = daemon --verbose --export-all --base-path=.git --reuseaddr --strict-paths .git/
[http]
	sslverify = true
[core]
	autocrlf = input
[pull]
	rebase = true
[credential]
	helper = store
[mergetool "gitblade"]
	cmd = \"/usr/lib/gitblade/gitblade\" -c \"\"
	trustExitCode = true
[cola]
	spellcheck = false
[url "ssh://git@github.com/jlighter"]
	insteadOf = https://github.com/jlighter
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[url "git@github.com:"]
	insteadOf = https://github.com/
